<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>Show Users</h1>
      <table class="table">
        <tr>
          <td>User Id</td>
          <td>First Name</td>
          <td>Last Name</td>
          <td>Email Address</td>
        </tr>	
        <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
    	<td><%= user.first_name %></td>
    	<td><%= user.last_name %></td>
    	<td><%= user.email %></td>
          </tr>
      <% end %>	
    </table>
  </div>
</div>